.userinfoContainer {
  background-color: #0094aa;
  width: 100%;
  height: 300rpx;
}
.userinfoContainer .userinfoContainer {
  display: flex;
}
.userinfoContainer .userinfoContainer .userIcon {
  flex: 1;
  border-radius: 50%;
  width: 150rpx;
  justify-content: center;
  align-items: center;
}
.userinfoContainer .userinfoContainer .userName {
  flex: 2;
  justify-content: center;
  align-items: center;
}
